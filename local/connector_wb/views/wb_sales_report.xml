<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id='wb_sales_report_action' model='ir.actions.act_window'>
        <field name="name">Sales report on sales</field>
        <field name="res_model">wb.sales.report</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem name="Sales report on sales" id="wb_sales_report_menu" parent="wb_base_menu" action="wb_sales_report_action"/>

    <record id="wb_sales_report_view_tree" model="ir.ui.view">
        <field name="name">Sales report on sales</field>
        <field name="model">wb.sales.report</field>
        <field name="arch" type="xml">
            <tree>
                <field name="realizationreport_id"/>
                <field name="suppliercontract_code"/>
            </tree>
        </field>
    </record>

</odoo>