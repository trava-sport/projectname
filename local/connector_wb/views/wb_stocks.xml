<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id='wb_stocks_action' model='ir.actions.act_window'>
        <field name="name">Stocks service</field>
        <field name="res_model">wb.stocks</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem name="Stocks service" id="wb_stocks_menu" parent="wb_base_menu" action="wb_stocks_action"/>

    <record id="wb_stocks_view_tree" model="ir.ui.view">
        <field name="name">Stocks service</field>
        <field name="model">wb.stocks</field>
        <field name="arch" type="xml">
            <tree>
                <field name="last_change_date"/>
                <field name="supplier_article"/>
                <field name="tech_size"/>
                <field name="barcode"/>
                <field name="quantity"/>
                <field name="is_supply"/>
                <field name="is_realization"/>
                <field name="quantity_full"/>
                <field name="quantity_not_in_orders"/>
                <field name="warehouse_name"/>
                <field name="in_way_to_client"/>
                <field name="in_way_from_client"/>
                <field name="nmid"/>
                <field name="subject"/>
                <field name="category"/>
                <field name="days_on_site"/>
                <field name="brand"/>
                <field name="sccode"/>
                <field name="price"/>
                <field name="discount"/>
            </tree>
        </field>
    </record>

</odoo>